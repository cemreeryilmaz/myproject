package metotlar;

import java.util.Arrays;

public class Uygulama6 {

	public static void main(String[] args) {
		int[]dizi=new int[10];
		for (int i = 0; i < dizi.length; i++) {
			dizi[i]=(int) (Math.random()*100);
		}
		int[]sýralanmýþdizi=diziSýrala(dizi);
		for (int i : sýralanmýþdizi) {
			System.out.print(i+"\t");
		}
	}
	private static int[] diziSýrala(int[] dizi) {
		Arrays.sort(dizi);
		return dizi;
	}
}
