package operatörler;

public class Uygulama2 {

	public static void main(String[] args) {
		byte sayý1=10;
		byte sayý2=10;
		byte artýönde=++sayý1;
		byte artýsonda=sayý2++;
		System.out.println("Artý öndeyken:"+artýönde);
		System.out.println("Artý sondayken:"+artýsonda);
		System.out.println("Sayý1 in son hali:"+sayý1);
		System.out.println("Sayý2 nin son hali:"+sayý2);

	}

}
