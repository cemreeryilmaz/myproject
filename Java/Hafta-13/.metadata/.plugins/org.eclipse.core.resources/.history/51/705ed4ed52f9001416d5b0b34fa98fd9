package içiçesýnýflar;

public class Çalýþan {
	
	Adres adres=new Adres();
	
	public class Adres{
		
		int binano,daireno;
		String sokakadý,caddeadý;
		
		public void adresBelirle(String sa,String ca,int bn,int dn){
			this.binano=bn;
			this.caddeadý=ca;
			this.daireno=dn;
			this.sokakadý=sa;
		}
		
	}

}
