package statickullanýmý;

public class Uygulama3 {

	public static void main(String[] args) {
		Çalýþan çalýþan=new Çalýþan(1234, "Ahmet", "Yýlmaz", 2000);
		System.out.println("Çevrimden önce sigortano:"+çalýþan.sigortano);
		System.out.println("Çevrimden önce ad:"+çalýþan.ad);
		System.out.println("Çevrimden önce soyad:"+çalýþan.soyad);
		System.out.println("Çevrimden önce maaþ:"+çalýþan.maaþ);
		Öðretmen öðretmen=(Öðretmen)çalýþan;
		System.out.println("Aþaðý çevrim yapýldýktan sonra:");
		System.out.println("Çevrimden sonra sigortano:"+öðretmen.sigortano);
		System.out.println("Çevrimden sonra ad:"+öðretmen.ad);
		System.out.println("Çevrimden sonra soyad:"+öðretmen.soyad);
		System.out.println("Çevrimden sonra maaþ:"+öðretmen.maaþ);
		öðretmen.branþ="Tarih";
		System.out.println("Çevrimden sonra branþ:"+öðretmen.branþ);
		

	}

}
