package diziler;

import java.util.Scanner;

public class Uygulama3 {

	public static void main(String[] args) {
		Scanner klavye=new Scanner(System.in);
		int []dizi=new int[7];
		for (int i = 0; i < dizi.length; i++) {//Diziye eleman yerleþtirme
			System.out.println(i+1+".nci sayýyý giriniz:");
			dizi[i]=klavye.nextInt();
		}//Sýralama Baþlangýç
		for (int i = 0; i < dizi.length; i++) {
			for (int j = 0; j < dizi.length-1; j++) {
				if(dizi[j]>dizi[j+1]){
					int yedek=dizi[j];
					dizi[j]=dizi[j+1];
					dizi[j+1]=yedek;
				}
			}
		}//Sýralama Bitiþ
		//Konsolda gösterme iþlemi.
		for (int i = 0; i < dizi.length; i++) {
			System.out.print(dizi[i]+"\t");
		}
	}
}
