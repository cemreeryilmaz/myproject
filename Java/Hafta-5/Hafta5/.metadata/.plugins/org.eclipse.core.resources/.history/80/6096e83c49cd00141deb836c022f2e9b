package diziler;

import java.util.Scanner;

public class Uygulama2 {

	public static void main(String[] args) {
		Scanner klavye=new Scanner(System.in);
		System.out.println("Kaç tane isim girmek istiyorsunuz?:");
		int elemansayýsý=klavye.nextInt();
		String[]isimler=new String[elemansayýsý];
		for (int i = 0; i < isimler.length; i++) {
			System.out.println(i+1+".nci ismi giriniz:");
			isimler[i]=klavye.nextLine();
		}
		for (String adlar : isimler) {
			System.out.println(adlar);
		}
	}
}
